                       -> No enumerado -> NumeroPuerto -> Que_puedo_enumerar?
                                       -> NumeroPuerto -> Que_puedo_enumerar?

Para intentar -> Puertos -> Funciono -> NumeroPuerto -> Descripción_acción_realizada -> Descripción_resultado_obtenido 
                                     -> NumeroPuerto -> Descripción_acción_realizada -> Descripción_resultado_obtenido 
                         -> No funciono -> NumeroPuerto -> Descripción_acción_realizada -> Descripción_resultado_obtenido
                                        -> NumeroPuerto -> Descripción_acción_realizada -> Descripción_resultado_obtenido
                         -> No intente -> NumeroPuerto -> Que_puedo_intentar?
                                       -> NumeroPuerto -> Que_puedo_intentar?

Explote -> Descripción_explotación -> Descripción_tipo_de_acceso_obtenido

